{
  "name": "valhalla",
  "version": "0.1.0",
  "license": "MIT",
  "private": false,
  "scripts": {
    "copy:idl": "cp ./target/idl/valhalla.json ./app/src/program/idl.json",
    "copy:types": "cp ./target/types/valhalla.ts ./app/src/program/valhalla.ts",
    "init-locker:devnet": "anchor run init-locker --provider.cluster devnet",
    "init-locker:mainnet": "anchor run init-locker --provider.cluster mainnet-beta",
    "test": "anchor test",
    "deploy:devnet": "anchor deploy --provider.cluster devnet",
    "deploy:mainnet": "anchor deploy --provider.cluster mainnet-beta",
    "deploy:devnet:init": "anchor deploy --provider.cluster devnet && yarn init-locker:devnet && yarn solita && yarn copy:idl && yarn copy:types",
    "deploy:mainnet:init": "anchor deploy --provider.cluster mainnet-beta && yarn init-locker:mainnet && yarn solita && yarn copy:idl && yarn copy:types",
    "lint:fix": "prettier */*.js \"*/**/*{.js,.ts}\" -w",
    "lint": "prettier */*.js \"*/**/*{.js,.ts}\" --check"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.29.0",
    "@metaplex-foundation/beet-solana": "^0.4.1",
    "@metaplex-foundation/js": "^0.20.1",
    "@metaplex-foundation/kinobi": "^0.16.15",
    "@metaplex-foundation/mpl-token-metadata": "^1.2.5",
    "@metaplex-foundation/solita": "^0.20.1",
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@solana/spl-token": "^0.3.9",
    "@types/bn.js": "^5.1.0",
    "@types/chai": "^4.3.11",
    "@types/jest": "^29.0.3",
    "@types/mocha": "^10.0.6",
    "chai": "^4.3.10",
    "jest": "^29.0.3",
    "mocha": "^10.2.0",
    "prettier": "^2.6.2",
    "ts-jest": "^29.0.2",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^4.3.5"
  }
}
