{
  "name": "valhalla",
  "version": "0.1.0",
  "license": "MIT",
  "private": false,
  "scripts": {
    "copy": "cp ./target/idl/valhalla.json ./app/src/program/idl.json && cp ./target/types/valhalla.ts ./app/src/program/valhalla.ts && cp ./target/idl/valhalla.json ./server/src/program/idl.json && cp ./target/types/valhalla.ts ./server/src/program/valhalla.ts",
    "test": "anchor test",
    "deploy:local": "anchor deploy --provider.cluster localnet",
    "deploy:devnet": "anchor deploy --provider.cluster devnet",
    "deploy:mainnet": "anchor deploy --provider.cluster mainnet-beta",
    "init:local": "anchor run init --provider.cluster localnet",
    "init:devnet": "anchor run init --provider.cluster devnet",
    "init:mainnet": "anchor run init --provider.cluster mainnet-beta",
    "deploy:init:local": "anchor deploy --provider.cluster localnet && yarn init:local && yarn solita && yarn copy",
    "deploy:init:devnet": "anchor deploy --provider.cluster devnet && yarn init:devnet && yarn solita && yarn copy",
    "deploy:init:mainnet": "anchor deploy --provider.cluster mainnet-beta && yarn init:mainnet && yarn solita && yarn copy",
    "create-data:local": "anchor run create-data --provider.cluster localnet",
    "create-data:devnet": "anchor run create-data --provider.cluster devnet",
    "lint:fix": "prettier */*.js \"*/**/*{.js,.ts}\" -w",
    "lint": "prettier */*.js \"*/**/*{.js,.ts}\" --check"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.29.0",
    "@metaplex-foundation/beet-solana": "^0.4.1",
    "@metaplex-foundation/js": "^0.20.1",
    "@metaplex-foundation/kinobi": "^0.16.15",
    "@metaplex-foundation/mpl-token-metadata": "^3.2.1",
    "@metaplex-foundation/solita": "^0.20.1",
    "@metaplex-foundation/umi": "^0.9.1",
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@solana/spl-token": "^0.3.9",
    "@types/bn.js": "^5.1.0",
    "@types/chai": "^4.3.11",
    "@types/jest": "^29.0.3",
    "@types/mocha": "^10.0.6",
    "chai": "^4.3.10",
    "jest": "^29.0.3",
    "mocha": "^10.2.0",
    "prettier": "^2.6.2",
    "ts-jest": "^29.0.2",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@types/project-name-generator": "^2.1.3",
    "project-name-generator": "^2.1.9"
  }
}
